class Struct{
	int x;
	int y;
	
	Struct(){
		System.out.println("기본 생성자");
	}
	
	Struct(int x, int y){
		
		this.x = x;
		this.y = y;
		
		System.out.println("x : " + x + " , y : " + y);
	}
}
