package designPattern.java;


class A{
	protected void printClass(){
		System.out.println("1");
		System.out.println(getClass());
	}
}

class B extends A{
	void print(){
		System.out.println("2");
		printClass();
	}
}

public class Test2 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		B b = new B();
		b.print();
	}
}
